{
  "name":"Copy Command",
  "description":"http://stackoverflow.com/questions/14171654/chrome-extension-how-to-select-all-text-of-tab-and-copy",
  "version":"1",
  "manifest_version":2,
  "browser_action": {"default_icon":"icon.png"},
  "background":{
    "page":"background.html"
  },
  "permissions":
    [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"], 
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["oncopy.js"]
    }
  ]
}
